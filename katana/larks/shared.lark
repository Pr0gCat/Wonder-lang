SPACES: (" " | /\t/)+
_NL: (/\r?\n/)+
COMMENT: /#.*\n/
MULTILINE_STRING: /\"\"\"[.\n\s]*\"\"\"/

IDENT: /[_a-zA-Z][\w]*/

// Literals
SIMPLE_STRING: /"(?:\\["\\tnr0]|[^\n"\\])*"/
STRING: /[a-z]?"(?:\\["\\tnr0]|[^\n"\\])*"/
CHARACTER: /'(?:.?|\\["'\\a-z])'/
HEX_NUM: /0x[0-9a-fA-F_]*/
BIN_NUM: /0b[10_]+/
SCI_NOTATION_NUM: /(?:\d*\.?\d+)[eE][-]?\d+/
FLOATING_NUM: /\d*\.\d+/
DEC_NUM: /0|[1-9]\d+/
